    <script>
      document.addEventListener('DOMContentLoaded', () => {
        const heroSection = document.querySelector('.hero-section');
        const images = document.querySelectorAll('.parallax-img');

        if (!heroSection || images.length === 0) return;

        heroSection.addEventListener('mousemove', (e) => {
          const { clientX, clientY } = e;
          const { innerWidth, innerHeight } = window;

          // Center of the screen
          const centerX = innerWidth / 2;
          const centerY = innerHeight / 2;

          // Calculate offset from center
          const xPos = clientX - centerX;
          const yPos = clientY - centerY;

          images.forEach(img => {
            const speed = parseFloat(img.getAttribute('data-speed'));

            // For the centered image (p-img-1), we need to maintain the -50% translate
            let baseTransform = '';
            if (img.classList.contains('p-img-1')) {
              baseTransform = 'translate(-50%, -50%)';
            }

            const xOffset = xPos * speed;
            const yOffset = yPos * speed;

            img.style.transform = `${baseTransform} translate3d(${xOffset}px, ${yOffset}px, 0)`;
          });
        });

        // Optional: Reset effect on mouse leave (smoothly)
        heroSection.addEventListener('mouseleave', () => {
          images.forEach(img => {
            let baseTransform = '';
            if (img.classList.contains('p-img-1')) {
              baseTransform = 'translate(-50%, -50%)';
            }
            // Reset to original position
            img.style.transform = `${baseTransform} translate3d(0, 0, 0)`;
          });
        });
      });
    </script>
